# top-most EditorConfig file
root = true

[*]
end_of_line = lf
insert_final_newline = true
charset = utf-8

[*.json]
indent_style = space
indent_size = 2

[.vscode/*.json]
indent_style = space
indent_size = 4

[*.{js,ts}]
indent_style = space
indent_size = 4

[*.{yml,yaml}]
indent_style = space
indent_size = 2
